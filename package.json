{
  "name": "gatsby-starter-tailwind-lp",
  "private": true,
  "description": "Gatsby starter for landing page with Emotion and tailwind. Linting with ESLint and stylelint.",
  "version": "0.1.0",
  "author": "tombo gokuraku",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
